    "license": "GPL-3.0-or-later",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vendure-ecommerce/vendure"
    },
    "scripts": {
        "build": "rimraf dist && tsc -p ./tsconfig.build.json",
        "watch": "tsc -p ./tsconfig.build.json --watch",
