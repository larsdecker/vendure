        "test:e2e": "vitest --config ./test/e2e/vitest.config.mts --run --dir test/e2e"
    },
    "homepage": "https://www.vendure.io",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "graphql-tag": "^2.12.6"
    },
    "devDependencies": {
        "@vendure/common": "3.5.0",
        "@vendure/core": "3.5.0",
        "@vendure/testing": "3.5.0",
        "nock": "^13.5.0",
        "node-fetch": "^3.3.2",
        "rimraf": "^5.0.5",
        "typescript": "5.8.2"
    }
}
